#MiddleButton=rox #light
#MiddleButton=grun #heavy

####Top-Right: RMB: Utility:#####
#### uncomment JUST ONE #####
#RightButton=xterm #tiny
#RightButton=xterm #light
RightButton=spacefm #heavy


